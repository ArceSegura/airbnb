{
  "extends": "next/core-web-vitals",
  "rules": {
    "indent": [
      "error",
      2
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ],
    "arrow-parens": [2, "as-needed", { "requireForBlockBody": true }],
    "template-curly-spacing" : "off",
    "no-param-reassign": 0,
    "no-duplicate-imports": "error",
    "camelcase": "error",
    "eqeqeq": ["error", "always"],
    "no-return-await": "error"
  }
}
